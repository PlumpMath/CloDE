<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Color x:Key="HighlightDarkColor">#FF9e5608</Color>
  <Color x:Key="HighlightLightColor">#FF9e5608</Color>
  <Color x:Key="AccentDarkColor">#FFde872a</Color>
  <Color x:Key="AccentColor">#CCff9829</Color>
  <Color x:Key="AccentColor2">#99ff9829</Color>
  <Color x:Key="AccentColor3">#66ff9829</Color>
  <Color x:Key="AccentColor4">#33ff9829</Color>
  <LinearGradientBrush x:Key="BrandingBrush" EndPoint="0.001,0.5" StartPoint="1.002,0.5">
    <GradientStop Color="{DynamicResource HighlightDarkColor}" Offset="0" />
    <GradientStop Color="#FF5FC316" Offset="1" />
  </LinearGradientBrush>
  <SolidColorBrush x:Key="AccentColorBrush" Color="{DynamicResource AccentColor}" />
  <!-- accent color on top of the tab -->
  <SolidColorBrush x:Key="AvalonDock_ThemeMetro_BaseColor3" Color="{DynamicResource AccentColor}" />
  <!-- accent color on top of the tab for mouse over -->
  <SolidColorBrush x:Key="AvalonDock_ThemeMetro_BaseColor4" Color="{DynamicResource AccentColor4}" />

</ResourceDictionary>
